<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>PrintRecords</servlet-name>
        <servlet-class>com.lukoseviciute.programming.web.PrintRecords</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PrintRecords</servlet-name>
        <url-pattern>/csvrecords</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PrintFromDB</servlet-name>
        <servlet-class>com.lukoseviciute.programming.web.PrintFromDB</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PrintFromDB</servlet-name>
        <url-pattern>/dbrecords</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HammerWomen</servlet-name>
        <servlet-class>com.lukoseviciute.programming.web.HammerWomen</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>HammerWomen</servlet-name>
        <url-pattern>/athlete-list</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>HammerWomen</servlet-name>
        <url-pattern>/insert</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>HammerWomen</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>HammerWomen</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>







</web-app>